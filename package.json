{
  "name": "visa-services",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "set NODE_ENV=production&& node server.js",
    "dev:dev": "env-cmd -f .env.dev next dev",
    "dev:prod": "env-cmd -f .env.prod next dev",
    "start:dev": "env-cmd -f .env.dev next dev",
    "start:prod": "env-cmd -f .env.prod next dev",
    "build:dev": "env-cmd -f .env.dev next build",
    "build:prod": "next build",
    "lint": "next lint",
    "prisma:init": "env-cmd -f .env.dev -- npx prisma migrate dev --name init",
    "migrate:dev": "env-cmd -f .env.dev -- npx prisma migrate dev",
    "generate:dev": "env-cmd -f .env.dev -- npx prisma generate",
    "migrate:prod": "env-cmd -f .env.prod -- npx prisma migrate deploy",
    "generate:prod": "env-cmd -f .env.prod -- npx prisma generate",
    "postinstall": "npx prisma generate"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.3",
    "@mui/material": "^6.1.2",
    "@mui/x-date-pickers": "^7.20.0",
    "@prisma/client": "^5.20.0",
    "@reduxjs/toolkit": "^2.3.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.3",
    "dayjs": "^1.11.13",
    "env-cmd": "^10.1.0",
    "formik": "^2.4.6",
    "http": "^0.0.1-security",
    "http-status": "^1.7.4",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.2.14",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "url": "^0.11.4",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/mocha": "^10.0.9",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.14",
    "prisma": "^5.20.0",
    "typescript": "^5"
  }
}
